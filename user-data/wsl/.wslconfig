# More at: https://learn.microsoft.com/en-us/windows/wsl/wsl-config
[wsl2]
pageReporting=true
guiApplications=true
nestedVirtualization=true
vmIdleTimeout=1800000
networkingMode=mirrored
firewall=true
dnsTunneling=true
defaultVhdSize=32GB
[experimental]
autoMemoryReclaim=gradual
hostAddressLoopback=true
[boot]
command=""
[automount]
enabled=false
[network]
generateHosts=true
generateResolvConf=true
[gpu]
enabled=true
[time]
useWindowsTimezone=true
[user]
default=keroack